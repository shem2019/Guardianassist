<!-- res/drawable/ic_tag_nfc.xml -->
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <!-- Tag background: white fill with black outline -->
    <path
        android:fillColor="#FFFFFFFF"
        android:strokeColor="#FF000000"
        android:strokeWidth="2"
        android:pathData="M2,2 H14 L20,8 V22 H2 Z"/>

    <!-- NFC inner wave (smaller) -->
    <path
        android:fillColor="#00FFFF"
        android:strokeColor="#FF000000"
        android:strokeWidth="1.5"
        android:pathData="M9,11
                          c1.1,1.1 1.1,2.9 0,4"/>

    <!-- NFC outer wave (larger) -->
    <path
        android:fillColor="#00FFFF"
        android:strokeColor="#FF000000"
        android:strokeWidth="1.5"
        android:pathData="M12,8
                          c2.21,2.21 2.21,5.79 0,8"/>
</vector>
